<!DOCTYPE html>
<html>
<head>
<title>User</title>
<link rel="stylesheet" type="text/css" href="css/UserManagerCSS.css" />
</head>
<body>

	<div id="divTable">
		<fieldset id="fieldset">
			<legend>User Manager </legend>
			<div id="divadd">
				<table id="tabuser">
					<tr>
						<th>Action</th>
						<th>Name</th>
						<th>Email</th>
						<th>Phone</th>
						<th>Website</th>
					</tr>
				</table>
			</div>
			<div>
				<input id="btnAdd" type="button" value="addUser" />
			</div>
		</fieldset>
	</div>
	<div id="overlay"></div>
	<div id="popup">
		<fieldset id=popFiledset>
			<legend id="formTitle">Details of the User</legend>
			<form>
				<div>
					<label for="txtName">Name</label> <input id="txtName" type="text"
						name="name" placeholder="Name" />
				</div>
				<div>
					<label for="txtEmail">Email</label> <input id="txtEmail"
						type="Email" name="email" placeholder="Email" />
				</div>
				<div>
					<label for="txtPhone">Phone</label> <input id="txtPhone" type="text"
						name="phone" placeholder="Phone" />
				</div>
				<div>
					<label for="txtWeb">Website</label> <input id="txtWebsite" type="text"
						name="website" placeholder="Website" />
				</div>
				<div>
					<input id="btnSave" type="button" value="Save" />
				
					<input id="btnUpd" type="button" value="Update" />
					
					<input id="btnCal" type="button" value="Cancel" />
				</div>

			</form>
		</fieldset>
	</div>
	<script src="javascript/jquery.js" ></script>
	<script src="javascript/UserManagerJS.js"></script>
</body>
</html>